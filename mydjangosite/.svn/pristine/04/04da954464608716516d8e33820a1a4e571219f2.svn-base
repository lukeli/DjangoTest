{% load flatpages %}
{% get_flatpages as flatpages %}
<ul>
    {% for page in flatpages %}
        <li><a href="{{ page.url }}">{{ page.title }}</a></li>
    {% endfor %}
</ul>

<html>
<head>
    <title>My Site: {{ flatpage.title }}</title>
</head>
<body>
<h1>{{ flatpage.title }}</h1>
<p>{{ flatpage.content }}</p>
</body>
</html>