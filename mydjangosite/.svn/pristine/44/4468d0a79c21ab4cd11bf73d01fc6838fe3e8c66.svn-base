{% extends "pastebin/base.html" %}
{% block title %}Recently Pasted{% endblock %}
{% block content %}
<h1>Recently Pasted</h1>
<ul>
    {% for object in object_list|slice:":5" %}
    <li><a href="{{ object.get_absolute_url }}">{{ object }}</a> &nbsp;&nbsp; <a href= "./edit/{{ object.id }}/">Edit</a>   </li>
    {% endfor %}
</ul>
{% endblock %}
