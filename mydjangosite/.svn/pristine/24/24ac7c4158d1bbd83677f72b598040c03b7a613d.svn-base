{% extends "./photoGallery/base.html" %}

{% block title %}{{ photo.item.name }} - {{ photo.title }}{% endblock %}

{% block content %}

<a href="{{ photo.item.get_absolute_url }}">&laquo; Back to {{ photo.item.name }} detail page</a>

<h2>{{ photo.item.name }} - {{ photo.title }}</h2>
<img src="{{ photo.image.url }}" heights = "400" width="500"  />
{% if photo.caption %}<p>{{photo.caption }}</p>{% endif %}


{% endblock %}
